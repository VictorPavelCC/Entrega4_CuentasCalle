<div>
    <h1>Productos</h1>
    {{#each products}}
    <div>
        <h2>{{this.title}}</h2>
        <p>{{this.description}}</p>
        <p>{{this.code}}</p>
        <p>{{this.price}}</p>
        <p>{{this.status}}</p>
        <p>{{this.stock}}</p>
        <p>{{this.category}}</p>
        <p>{{this.thumbnail}}</p>
    </div>
    {{/each}}
</div>